{
  "name": "@flow-connect/audio",
  "version": "1.0.3",
  "description": "Standard Audio nodes collection for FlowConnect",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/cjs/index.js",
  "type": "module",
  "types": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saurabh-prosoft/flow-connect-standard-nodes.git",
    "directory": "packages/audio"
  },
  "bugs": {
    "url": "https://github.com/saurabh-prosoft/flow-connect-standard-nodes/issues"
  },
  "author": "saurabh-prosoft <saurabhbhagat98die@gmail.com>",
  "homepage": "https://flow-connect.saurabhagat.me/reference/standard-nodes/audio.html",
  "license": "MIT",
  "keywords": [
    "flow-connect",
    "standard-nodes",
    "audio",
    "standard-audio",
    "flow-connect-audio"
  ],
  "scripts": {
    "prebuild": "npx shx rm -rf dist",
    "build": "npm run build:umd && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:umd": "webpack --config=webpack.prod.js",
    "build:cjs": "tsc",
    "build:esm": "tsc -m es6 --outDir ./dist/esm",
    "build:types": "tsc --outDir ./dist/types --declaration true --emitDeclarationOnly true",
    "prepublishOnly": "npm run build"
  },
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./*": {
      "import": "./dist/esm/*",
      "require": "./dist/cjs/*"
    },
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/index.d.ts"
      ]
    }
  },
  "browser": "./dist/audio.js",
  "peerDependencies": {
    "flow-connect": "^1.0.12"
  }
}
